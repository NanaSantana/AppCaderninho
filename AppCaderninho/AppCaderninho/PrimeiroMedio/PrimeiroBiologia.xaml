<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppCaderninho.PrimeiroMedio.PrimeiroBiologia">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Origem da Vida, Citologia, Reprodução, Embriologia, Histologia, Seres Vivos, Genética, Evolução e Ecologia."
                HorizontalOptions="CenterAndExpand"
                Margin="10"/>
            <Image x:Name="gatoBiologia"
                   WidthRequest="100"
                   HeightRequest="500"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>